{
  "extends": ["tslint:recommended"],
  "rules": {
    "interface-name": false,
    "only-arrow-functions": false,
    "quotemark": false,
    "arrow-parens": false,
    "no-empty-interface": false,
    "member-access": [true, "no-public"],
    "variable-name": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
